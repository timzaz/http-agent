{
  "author": "A. J. Wray",
  "bugs": {
    "url": "https://github.com/timzaz/http-agent/issues"
  },
  "dependencies": {
    "superagent": "^10.1.1"
  },
  "description": "Wrapper functions around superagent",
  "devDependencies": {
    "@types/superagent": "^8.1.9",
    "typescript": "^5.7.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./interfaces": {
      "types": "./dist/interfaces.d.ts",
      "default": "./dist/interfaces.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/timzaz/http-agent#readme",
  "keywords": [
    "superagent",
    "fetch",
    "api"
  ],
  "license": "GPL-3.0-or-later",
  "main": "./dist/index.js",
  "name": "@timzaz-ajw/http-agent",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ],
      "interfaces": [
        "dist/interfaces.d.ts"
      ],
      "utils": [
        "dist/utils.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timzaz/http-agent.git"
  },
  "scripts": {
    "build": "tsc",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "git add -A"
  },
  "version": "1.0.3"
}
